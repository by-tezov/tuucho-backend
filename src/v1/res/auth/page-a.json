{
  "setting": {
    "ttl": {
      "strategy": "transient",
      "transient-value": "10s"
    }
  },
  "root": {
    "setting": {
      "navigation": {
        "extra": {
          "is-background-solid": true
        },
        "definition": [
          {
            "selector": {
              "type": "page-bread-crumb",
              "value": "page-c"
            },
            "transition": {
              "type": "slide-vertical",
              "entrance": "from-bottom",
              "effect": "cover"
            }
          },
          {
            "option": {
              "single": true,
              "reuse": true
            },
            "transition": "slide-horizontal"
          }
        ]
      }
    },
    "subset": "layout-linear",
    "style": {
      "orientation": "vertical",
      "background-color": "*background@sunny-yellow",
      "fill-max-size": true
    },
    "content": {
      "items": [
        {
          "subset": "label",
          "style": "*title-label",
          "content": {
            "value": "Page-A"
          }
        },
        "*item-spacer",
        {
          "subset": "label",
          "style": "*title-body",
          "content": {
            "value": "Page TLL of 10s"
          }
        },
        "*item-spacer",
        {
          "subset": "layout-linear",
          "style": {
            "orientation": "horizontal",
            "background-color": "*background@funny-color",
            "fill-max-width": true
          },
          "content": {
            "items": [
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": { "id": "*label-main-button-back" },
                    "content": {
                      "value": "BACK"
                    }
                  },
                  "action": "navigate://local-destination/back"
                }
              },
              "*item-spacer",
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": { "id": "*label-main-button-back" },
                    "content": {
                      "value": "Page-B"
                    }
                  },
                  "action": "navigate://url/auth/page-b"
                }
              }
            ]
          }
        }
      ]
    }
  },
  "components": [
    {
      "id": "item-spacer-half",
      "subset": "spacer",
      "style": {
        "weight": "0.5"
      }
    }
  ]
}
