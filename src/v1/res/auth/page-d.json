{
  "root": {
    "setting": {
      "navigation": {
        "extra": {
          "is-background-solid": false
        },
        "definition": {
          "option": { "single": true },
          "transition": {
            "type": "slide-vertical",
            "entrance": "from-top",
            "effect": "cover"
          }
        }
      }
    },
    "subset": "layout-linear",
    "style": {
      "orientation": "vertical",
      "background-color": "*background@aqua-splash",
      "fill-max-size": true
    },
    "content": {
      "items": [
        "*item-spacer",
        {
          "subset": "label",
          "style": "*title-label",
          "content": {
            "value": "Page-D - with not solid background"
          }
        },
        "*item-spacer",
        {
          "subset": "layout-linear",
          "style": {
            "orientation": "horizontal",
            "background-color": "*background@funny-color",
            "fill-max-width": true
          },
          "content": {
            "items": [
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": "*label-main-button-default",
                    "content": {
                      "value": "BACK"
                    }
                  },
                  "action": "navigate://local-destination/back"
                }
              },
              "*item-spacer",
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": "*label-main-button-default",
                    "content": {
                      "value": "Page-E"
                    }
                  },
                  "action": "navigate://url/auth/page-e"
                }
              }
            ]
          }
        }
      ]
    }
  },
  "components": [
    {
      "id": "item-spacer-half",
      "subset": "spacer",
      "style": {
        "weight": "0.5"
      }
    }
  ]
}
