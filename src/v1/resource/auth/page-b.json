{
  "setting": {
    "time-to-live": {
      "strategy": "single-use"
    }
  },
  "root": {
    "setting": {
      "navigation": {
        "definitions": [
          {
            "selector": {
              "type": "page-bread-crumb",
              "value": "auth/page-e"
            },
            "option": {
              "single": true,
              "pop-up-to": {
                "url": "auth/page-e",
                "inclusive": false,
                "greedy": true
              }
            },
            "transition": {
              "type": "slide-horizontal",
              "entrance": "from-start",
              "effect": "cover-push"
            }
          },
          {
            "option": {
              "single": true
            },
            "transition": {
              "type": "slide-horizontal",
              "entrance": "from-start",
              "effect": "push"
            }
          }
        ]
      }
    },
    "subset": "layout-linear",
    "style": {
      "orientation": "vertical",
      "background-color": "*background@crazy-orange",
      "fill-max-size": true
    },
    "content": {
      "items": [
        {
          "subset": "label",
          "style": "*label@title",
          "content": {
            "value": "*title"
          }
        },
        "*spacer@full",
        {
          "subset": "label",
          "style": "*label@body",
          "content": {
            "value": "*body"
          }
        },
        "*spacer@full",
        {
          "subset": "layout-linear",
          "style": {
            "orientation": "horizontal",
            "background-color": "*background@funny-color",
            "fill-max-width": true
          },
          "content": {
            "items": [
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": {
                      "id": "*label@button-previous"
                    },
                    "content": {
                      "value": "*back"
                    }
                  },
                  "action": "*back"
                }
              },
              "*spacer@full",
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": "*label@button-next",
                    "content": {
                      "value": "*next"
                    }
                  },
                  "action": "*next"
                }
              }
            ]
          }
        }
      ]
    }
  },
  "texts": {
    "common": {
      "back": "back",
      "next": "next",
      "title": "Page-B",
      "body": "TLL single use not preloaded \n\n- nav stack single \n\n- transition horizontal from start push effect"
    }
  },
  "actions": {
    "common": {
      "back": "navigate://local-destination/back",
      "next": "navigate://url/auth/page-c"
    }
  }
}
