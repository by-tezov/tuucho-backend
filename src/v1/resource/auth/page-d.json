{
  "root": {
    "setting": {
      "navigation": {
        "extra": {
          "is-background-solid": false
        },
        "definitions": {
          "transition": {
            "type": "slide-vertical",
            "entrance": "from-top",
            "effect": "cover"
          }
        }
      }
    },
    "subset": "layout-linear",
    "style": {
      "orientation": "vertical",
      "background-color": "*background@aqua-splash",
      "fill-max-size": true
    },
    "content": {
      "items": [
        {
          "subset": "label",
          "style": "*label@title",
          "content": {
            "value": "*title"
          }
        },
        "*spacer@full",
        {
          "subset": "label",
          "style": "*label@body",
          "content": {
            "value": "*body"
          }
        },
        "*spacer@full",
        {
          "subset": "layout-linear",
          "style": {
            "orientation": "horizontal",
            "background-color": "*background@funny-color",
            "fill-max-width": true
          },
          "content": {
            "items": [
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": {
                      "id": "*label@button-previous"
                    },
                    "content": {
                      "value": "*back"
                    }
                  },
                  "action": "*back"
                }
              },
              "*spacer@full",
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": "*label@button-next",
                    "content": {
                      "value": "*next"
                    }
                  },
                  "action": "*next"
                }
              }
            ]
          }
        }
      ]
    }
  },
  "texts": {
    "common": {
      "back": "back",
      "next": "next",
      "title": "Page-D",
      "body": "TLL infinite \n\n- nav background not solid \n\n- transition vertical from top cover effect"
    }
  },
  "actions": {
    "common": {
      "back": "navigate://local-destination/back",
      "next": "navigate://url/auth/page-e"
    }
  }
}
