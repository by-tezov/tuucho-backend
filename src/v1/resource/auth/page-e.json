            "background-color": "*background@funny-color",
            "fill-max-width": true
          },
          "content": {
            "items": [
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": "*label-main-button-default",
                    "content": {
                      "value": "BACK"
                    }
                  },
                  "action": "navigate://local-destination/back"
                }
              }
            ]
          }
        }
      ]
    }
  }
