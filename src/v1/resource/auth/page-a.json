{
  "setting": {
    "time-to-live": {
      "strategy": "transient",
      "transient-value": "10s"
    }
  },
  "root": {
    "setting": {
      "navigation": {
        "extra": {
          "is-background-solid": true
        },
        "definitions": [
          {
            "selector": {
              "type": "page-bread-crumb",
              "value": "auth/page-c"
            },
            "transition": {
              "type": "slide-vertical",
              "entrance": "from-bottom",
              "effect": "cover"
            }
          },
          {
            "option": {
              "single": true,
              "reuse": true
            },
            "transition": "slide-horizontal"
          }
        ]
      }
    },
    "subset": "layout-linear",
    "style": {
      "orientation": "vertical",
      "background-color": "*background@sunny-yellow",
      "fill-max-size": true
    },
    "content": {
      "items": [
        {
          "subset": "label",
          "style": "*label@title",
          "content": {
            "value": "*title"
          }
        },
        "*spacer@full",
        {
          "subset": "label",
          "style": "*label@body",
          "content": {
            "value": "*body"
          }
        },
        "*spacer@full",
        {
          "subset": "layout-linear",
          "style": {
            "orientation": "horizontal",
            "background-color": "*background@funny-color",
            "fill-max-width": true
          },
          "content": {
            "items": [
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": {
                      "id": "*label@button-previous"
                    },
                    "content": {
                      "value": "*back"
                    }
                  },
                  "action": "*back"
                }
              },
              "*spacer@full",
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": {
                      "id": "*label@button"
                    },
                    "content": {
                      "value": "*page-image"
                    }
                  },
                  "action": "*page-image"
                }
              },
              "*spacer@full",
              {
                "subset": "button",
                "content": {
                  "label": {
                    "style": "*label@button-next",
                    "content": {
                      "value": "*next"
                    }
                  },
                  "action": "*next"
                }
              }
            ]
          }
        }
      ]
    }
  },
  "texts": {
    "common": {
      "back": "back",
      "page-image": "Page-Image",
      "next": "next",
      "title": "Page-A",
      "body": "TLL transient of 10s \n\n- nav stack single + reuse \n\n- transition horizontal from end cover effect by default \n\n- transition cover vertical from bottom if come from page C"
    }
  },
  "actions": {
    "common": {
      "back": "navigate://local-destination/back",
      "page-image": "navigate://url/auth/page-image",
      "next": "navigate://url/auth/page-b"
    }
  }
}
